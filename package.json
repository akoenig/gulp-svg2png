    "test": "npm run build && npm run mocha"
  },
  "engines": {
	"node": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:akoenig/gulp-svg2png.git"
  },
  "keywords": [
    "gulpplugin",
    "svg",
    "png",
    "converter",
    "svg2png"
  ],
  "bugs": {
    "url": "https://github.com/akoenig/gulp-svg2png/issues"
  },
  "homepage": "https://github.com/akoenig/gulp-svg2png",
  "dependencies": {
    "gulp-util": "^3.0.7",
    "map-stream": "0.0.6",
    "map-stream-limit": "^1.1.0",
    "svg2png": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "imagesize": "^1.0.0",
    "mocha": "^2.2.5",
    "typings": "^0.8.1",
    "typescript": "^1.7.3"
  }
}
